{
   "plugins" : [
      "NeatHTMLFeatures",
      "NeatCanvasFeatures",
      "HideTrackLabels",
      "TADkit"
   ],
   "tracks" : [
      {
         "chunkSize" : 20000,
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "urlTemplate" : "seq/{refseq_dirpath}/{refseq}-",
         "category" : "Reference sequence",
         "type" : "SequenceTrack",
         "label" : "DNA",
         "key" : "Reference sequence"
      },
      {
         "autocomplete" : "all",
         "track" : "Genes",
         "style" : {
            "className" : "feature5",
            "subfeatureClasses" : {
               "CDS" : "transcript-CDS"
            },
            "arrowheadClass" : "transcript-arrowhead"
         },
         "key" : "genes",
         "feature" : [
            "gene"
         ],
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "urlTemplate" : "tracks/Genes/{refseq}/trackData.json",
         "compress" : 0,
         "label" : "Genes",
         "type" : "FeatureTrack",
         "category" : "Genes",
         "subfeatures" : true
      },
      {
         "style" : {
            "className" : "feature"
         },
         "style": {
             "color": "function( feature, variableName, glyphObject, trackObject ) {  return feature.get('name'); }"
         },
         "key" : "Chromatin Types",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "trackType" : null,
         "urlTemplate" : "tracks/Chromatin Types/{refseq}/trackData.json",
         "compress" : 0,
         "type" : "TADkit/View/Track/TADkitOverlay",
         "label" : "Chromatin Types"
      },
      {
         "key" : "restraints",
         "storeClass" : "TADkit/Store/SeqFeature/Restraints",
         "type" : "TADkit/View/Track/TADkitRestraints",
         "label" : "Restraints",
         "maxHeight":100,
         "style": {
             "showLabels": false,
             "color": "blue"
         },
         "onClick": {
            "action": "contentDialog",
            "content" : "function(track,feature,div) { return '<p><b>Value:'+feature.get('score')+'</b></p>';} "
         }
     },
   ],
   "names" : {
      "url" : "names/",
      "type" : "Hash"
   },
   "formatVersion" : 1
}
