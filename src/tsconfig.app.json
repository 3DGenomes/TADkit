{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "paths": {
      "@projects/*": ["app/projects/*"],
      "@workspace/*": ["app/workspace/*"],
      "genomics-lib": ["../dist/genomics-lib"],
      "genomics-lib/*": ["../dist/genomics-lib/*"],
      "three-lib": ["../dist/three-lib"],
      "three-lib/*": ["../dist/three-lib/*"]
    },
    "module": "es2015",
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "**/unused/*"
  ]
}
